

pipeline {
    agent any

    stages {
        stage('dockerfile') {
            steps {

                sh'''
                docker build -t pacmanimage1 .
                '''
            }
        }
        stage('test') {
            steps {
                sh '''
                npm test
                '''
            }
        }
        stage('image') {
            steps {
                sh '''
                docker tag pacmanimage1:1.7
                '''
            }
        }


    }
}